<div
  class="tab-pane fade show active"
  id="show-image"
  role="tabpanel"
  aria-labelledby="home-tab"
>
  <div class="row">
    <% item.imageId.forEach((image) => { %>
    <div class="col-md-4">
      <div class="card shadow mb-4 mt-2">
        <div class="card-body">
            <img
                src="<%= image.imageUrl%>"
                class="img-fluid"
                alt="Responsive image"
            />
        </div>
      </div>
    </div>
 <% }); %>
  </div>
</div>
